{
  "extends": ["airbnb-base", "prettier"],
  "plugins": [],
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off"
  }
}
